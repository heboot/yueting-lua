function removeDialogNode()
    local curScene = Sprite:getCurScene()
    local dialogNode = Sprite:findChild(curScene, 'dialogNode')
    if dialogNode ~= 0 then
        Sprite:removeChild(Sprite:getParent(dialogNode),dialogNode,1)
    end
end

