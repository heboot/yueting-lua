function updateFunc()
    local showText = ""
    if mobileVideoVerTable.flags == '1' and Config:get('updateTips_setting') == '1' then -- 提示升级
        if mobileVideoVerTable.desc and mobileVideoVerTable.desc ~= '' then
            showText = mobileVideoVerTable.desc
