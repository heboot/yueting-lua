            setNodeState(Sprite:findChild(item, 'imgNode' .. i), 0)
        end
        return
    end
    if subjectCount > 4*index+1 then
        loadImagefallPodcastData(Sprite:findChild(item, 'imgNode2'), 4*index+1)
    else
        Sprite:setRect(item, 0, 0, 686, 192)
		Log:write('index=======2')
        for i=2, 4 do
            setNodeState(Sprite:findChild(item, 'imgNode' .. i), 0)
        end
        return
    end
    if subjectCount > 4*index+2 then
        loadImagefallPodcastData(Sprite:findChild(item, 'imgNode3'), 4*index+2)
    else
        Sprite:setRect(item, 0, 0, 686, 192)
		Log:write('index=======2')
        for i=3, 4 do
            setNodeState(Sprite:findChild(item, 'imgNode' .. i), 0)
        end
        return
    end
    if subjectCount > 4*index+3 then
        loadImagefallPodcastData(Sprite:findChild(item, 'imgNode4'), 4*index+3)
    else
        Sprite:setRect(item, 0, 0, 686, 192)
		Log:write('index=======4')
        setNodeState(Sprite:findChild(item, 'imgNode4'), 0)
        return
    end
end

