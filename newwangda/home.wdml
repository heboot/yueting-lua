	 setNodeState(mainListviewNovelNode,0)--小说ListView数据
	 setNodeState(mainListviewPodcastNode,0)--播客ListView数据
 elseif msg==102 then
	 -- Log:write('leftRecommendData======')
	 leftData = Http:jsonDecode('leftData')
	 Log:write('leftData===222===',leftData.novelPostList)
	 LoadleftNovelPanoramaData()
	 LoadleftNovelListviewData()
 elseif msg==107 then
     podcastRecommendData = Http:jsonDecode('podcastRecommendData')
