--直播、电台、小说、播客四个按钮的点击事件
function functionBtnOnSelect(sprite)
    local item = Sprite:getParent(sprite)
    local index = List:getItemIndex(item)
	local reg = Reg:create(Reg.com_wondertek_mobileaudio.home)
	if index==0 then 
	Reg:setInteger(reg,'currentFlag',0)
	setNodeState(listNode,1) --电台point数据list
	setNodeState(listNovelNode,0)--小说point数据list
	setNodeState(listPodcastNode,0)--播客point数据list	
	setNodeState(mainRecPanorama,1)--电台头部Panorama数据
	setNodeState(mainRecNovelPanorama,0)--小说头部Panorama数据
	setNodeState(mainRecPodcastPanorama,0)--播客头部Panorama数据
	setNodeState(mainListviewRadioNode,1)--电台ListView数据
	setNodeState(mainListviewNovelNode,0)--小说ListView数据
	setNodeState(mainListviewPodcastNode,0)--播客ListView数据
	-- Sprite:setEnable(sprite, 0)
	elseif index==1 then
	Reg:setInteger(reg,'currentFlag',1)
	setNodeState(listNode,0)
	setNodeState(listNovelNode,1)
	setNodeState(listPodcastNode,0)
	setNodeState(mainRecPanorama,0)
	setNodeState(mainRecNovelPanorama,1)
	setNodeState(mainRecPodcastPanorama,0)
	setNodeState(mainListviewRadioNode,0)
	setNodeState(mainListviewNovelNode,1)
	setNodeState(mainListviewPodcastNode,0)
	elseif index==2 then
	Reg:setInteger(reg,'currentFlag',2)
	setNodeState(listNode,0)
	setNodeState(listNovelNode,0)
	setNodeState(listPodcastNode,1)
	setNodeState(mainRecPanorama,0)
	setNodeState(mainRecNovelPanorama,0)
	setNodeState(mainRecPodcastPanorama,1)
	setNodeState(mainListviewRadioNode,0)
	setNodeState(mainListviewNovelNode,0)
	setNodeState(mainListviewPodcastNode,1)
	end
	
	local n= List:getCurItem(mainBottomList)
    for i=0,2 do
    local item =List:getItem(mainBottomList,i)
    local functionBtn=Sprite:findChild(item,'functionBtn')
        if i==n then
            Sprite:setEnable(functionBtn,0)
            curIndex=i
        else
            Sprite:setEnable(functionBtn,1)
        end
    end
end

--MainListView的Item点击事件
function contentOnSelect(sprite)
	local reg = Reg:create(Reg.com_wondertek_mobileaudio.home)
	local flag = Reg:getInteger(reg,'currentFlag')
	if flag == 1 then
		--小说跳转
		local data = Sprite:getData(sprite)
		local reg = Reg:create('com_wondertek_mobileaudio_novel')
		Reg:setString(reg,'novelItemId',data)
		-- Reg:setInteger(reg, 'novelIsUrl',2) 
		Scene:go('MODULE:\\com_wondertek_mobileaudio\\novelItem.wdml',{freeDestScene=true})
	elseif flag == 0 then
		--电台跳转
		local data = Sprite:getData(sprite)
		Log:write('godiantai',data)
		local reg = Reg:create(Reg.com_wondertek_mobileaudio.audiolive)
		Reg:setString(reg,'audioplayData',data)
		Scene:go('MODULE:\\com_wondertek_mobileaudio\\playyue.wdml',{freeDestScene=true})
	elseif flag == 2 then
		--播客跳转
		local data = Sprite:getData(sprite)
		local reg = Reg:create('Reg.com_wondertek_mobileaudio_podcast.personalBoke')
		Reg:setString(reg,'bokeItemData',data)
		Reg:setInteger(reg,'isNodeId','1')
		Scene:go('MODULE:\\com_wondertek_mobileaudio\\podcastList.wdml',{freeDestScene=true})
	end
end

