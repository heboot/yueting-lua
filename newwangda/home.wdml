-- @brief 创建电台panorama的标识点
function createPanoramaForeground(list, count)
    Sprite:setProperty(list,"col",count)
	Log:write('count====2==',count,list)
    local s_w = 720
    local point_w = 35
    local offset = 0
    local _, y, _, h = Sprite:getRect(list)
    local w1 = point_w * count + offset * (count+1)
    local x1 = (s_w - w1) / 2
    List:removeAllItems(list, 1, 1)
    for i = 1, count do
        local item = Sprite:create('listitem', list)
        local img = Sprite:create('image', item)
		-- local shadow1 = Sprite:create('shadow', item)
		-- Sprite:setRect(shadow1, 0, 0, point_w, point_w)
		-- Sprite:setProperty(shadow1, 'color', '#FFE4B5')
        Sprite:setRect(item, 0, 0, point_w, point_w)
        Sprite:setProperty(item, 'extendstyle', '0110')
        Sprite:setRect(img, 0, 0, point_w, point_w)
        Sprite:setProperty(img, 'extendstyle', '0100')
        Sprite:setName(img, 'img')
        Sprite:setProperty(img, 'src', 'file://image/panorama_point_f.png')
        -- resChoose(img)
        Sprite:setProperty(img, 'style', 'center')
        List:addItem(list, item)
		Log:write('point_w=====11===',point_w)
    end
    Sprite:setRect(list, x1, y, w1, h)
    Sprite:setProperty(list, 'extendstyle', '1110')
    List:adjust(list)
    setPanoramaForeground(list, 0)
end

