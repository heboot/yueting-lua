function getCloseTime()
    local curTime = os.time()
	local closeTimeReg = Reg:create('com_wondertek_mobileaudio_closetime')
	local closeTime = Reg:getInteger(closeTimeReg,'closeTime')
	local timeValue = Reg:getInteger(closeTimeReg,'timeValue')
