	Log:write('timedCloseBtnOnSelect===stop',curTime,closeTime)
	if curTime == closeTime then
	    Timer:cancel(4443)
		Sprite:setProperty(closeTimeLbl,'text','')
		setSwitchPos(60)
		Log:write('timedCloseBtnOnSelect===stop')
		Player:pause()
	else
	    Sprite:setProperty(closeTimeLbl,'text',timeFormat2(timeValue-1))
		Timer:set(4443,1000,'getCloseTime')
	end
end

-- @brief 格式化时间
function timeFormat2(sec)
    Log:write('@@@@@@ timeFormat @@@@@,sec=',sec)
    if sec == nil then return '00:00' end
    local mm = math.floor(sec / 60) < 10 and '0' .. math.floor(sec / 60) or math.floor(sec / 60)
    local ss = math.mod(sec, 60) < 10 and '0' .. math.mod(sec, 60) or math.mod(sec, 60)
    local time = mm .. ':' .. ss
    return time
end

