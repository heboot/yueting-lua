        end
        bF2Disabled = 1
        Sprite:setProperty(dtextShadow, "text", showText)
        local _,_,_,adjustHeight = Sprite:getRect(dtextShadow)
        local dl = Dialog.layout
        DialogAdjust(adjustHeight)
        Dialog:show('有新版本更新', showText, 'ok_cancel', 'downloadPackage1', 'cancelDownloadPackage', '升  级', '取  消','checkOnSet2', '不再提醒', false)
        Dialog.layout = dl
    elseif mobileVideoVerTable.flags == '2' then -- 强制升级
        if mobileVideoVerTable.desc and mobileVideoVerTable.desc ~= '' then
            showText = mobileVideoVerTable.desc
        else
            showText = "检测到新的客户端版本，若不升级将无法继续使用，是否进行升级？"
