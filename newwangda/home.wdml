        Timer:set(TimerId.once,2000,"commonlocal_UI_exit")
    end
end

-- @brief 下载大升级安装包
function downloadPackage()
    -- 开始下载
    if not downloadPath then return end
    Log:write('mobileVideoVerTable.url============', mobileVideoVerTable.url)
    Download:deleteByName('defaultSkin', 1, true)
    Download:deleteByName('intallPackageName', 1, true)
    Download:append(downloadPath, 'intallPackageName', mobileVideoVerTable.url, true)
end

function downloadFinishCallback()
    -- 安装退出
    if not downloadPath then return end
    AppManager:installApp(downloadPath)
    -- 安装成功，重启时引擎将干掉module
    -- 清除cache
    --Http:stopNetwork()
    IO:dirRemove('CACHE:\\com_wondertek_mobilevideo3', 1)
    IO:dirCreate('CACHE:\\com_wondertek_mobilevideo3')
    IO:dirCreate('CACHE:\\com_wondertek_mobilevideo3\\image')
end

function downloadFailCallback()
    Dialog:show('提示', '下载失败，请稍后再试', 'ok', 'failToDo')
end

function failToDo()
    if mobileVideoVerTable.flags == '1' then
        cancelDownloadPackage()
    elseif mobileVideoVerTable.flags == '2' then
        commonlocal_UI_exit()
    end
end

function cancelDownloadPackage()
    if Dialog:isChecked() then
        Config:set('updateTips_setting', 0)
    else
        Config:set('updateTips_setting', 1)
    end
    requestOneHistoryData()
    bF2Disabled = nil
    removeDialogNode()
end

function commonlocal_UI_exit()
    local dirPath = string.gsub('CACHE:\\com_wondertek_mobilevideo3', '(.*)([^\\])$', '%1%2\\')
    local dir = OpenDirectory(dirPath..'*.*')
    if dir then
        for n = 0, table.maxn(dir) do
            if (dir[n].filename == '.' or dir[n].filename == '..') and dir[n].attr == 1 then
            else
                os.remove(dirPath..dir[n].filename)
            end
        end
    end
    Scene:exit()
end

function getLoginUpdateData()
    local loginData = Http:xmlDecode('index_login')
    Log:write('loginData', loginData)
    if 'table' ~= type(loginData) then
        return
    end
    for i=1, #loginData.body.response do
        if loginData.body.response[i].type == 'CheckVersionResult' then
            local checkVersionTable = loginData.body.response[i].component
            if 'table' == type(checkVersionTable) then
                if #checkVersionTable > 0 then
                    for j=1, #checkVersionTable do
                        if checkVersionTable[j].id == 'MobileVideo' then
                            return checkVersionTable[j]
                        end
                    end
                elseif #checkVersionTable == 0 and checkVersionTable.id then
                    if checkVersionTable.id == 'MobileVideo' then
                        return checkVersionTable
                    end
                end
            end
        end
    end
    return
end

