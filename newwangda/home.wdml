        bF2Exit = 1
        Sprite:setProperty(dtextShadow, "text", showText)
        local _,_,_,adjustHeight = Sprite:getRect(dtextShadow)
        local dl = Dialog.layout
        DialogAdjust(adjustHeight)
        Dialog:show('有新版本更新', showText, 'ok_cancel', 'downloadPackage2', 'commonlocal_UI_exit', '升  级', '取  消','checkOnSet2', '不再提醒', false)
        Dialog.layout = dl
    end
end

-- @brief 下载大升级安装包，提示升级
function downloadPackage1()
    local flashCard = System:getFlashCardName(1)
    local innerCard = System:getFlashCardName(0)
    if (flashCard and flashCard ~= "") or (innerCard and innerCard ~= "") then
        if flashCard and flashCard ~= "" then
            downloadPath = flashCard .. 'intallPackageName' .. Config:get('install_pkg_ext')
        else
            downloadPath = innerCard .. 'intallPackageName' .. Config:get('install_pkg_ext')
        end
        if Dialog:isChecked() then
            Config:set('updateTips_setting', 0)
        else
            Config:set('updateTips_setting', 1)
        end
        downloadPackage()
        DownloadDialog:show('升级包下载', '取  消', 'intallPackageName', 'downloadFinishCallback', 'downloadFailCallback', 'cancelDownloadPackage')
    else
        Tips:show("未找到存储卡")
    end
    bF2Disabled = nil
    removeDialogNode()
end

-- @brief 下载大升级安装包，强制升级
function downloadPackage2()
    local flashCard = System:getFlashCardName(1)
    local innerCard = System:getFlashCardName(0)
    if (flashCard and flashCard ~= "") or (innerCard and innerCard ~= "") then
        if flashCard and flashCard ~= "" then
            downloadPath = flashCard .. 'intallPackageName' .. Config:get('install_pkg_ext')
        else
            downloadPath = innerCard .. 'intallPackageName' .. Config:get('install_pkg_ext')
        end
        downloadPackage()
        DownloadDialog:show('升级包下载', '退  出', 'intallPackageName', 'downloadFinishCallback', 'downloadFailCallback', 'commonlocal_UI_exit')
